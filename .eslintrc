{
    "rules": {
        "indent": [
            2,
            2,
            {"VariableDeclarator": 2}
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-dangle": [
            2,
            "always-multiline"
        ],
        "no-console": 0
    },
    "globals": {
        "angular": false,
        "google": false
    },
    "env": {
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
